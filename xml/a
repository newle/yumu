                //bobo数据
            for (var i = 0; i < len_max; i++){
                alttitle = newData[i].anchorExtendTitle.replace(re, "&quot;");
                item_url = (newData[i].live == 1) ? newData[i].liveUrl : "http://bobo.163.com/";
                live_snip_html = (newData[i].live == 1) ? '<div class="livestat"></div>' : "";
                html.push('<li><a href="' + item_url + code + '"><div class="pic"><img src="' + newData[i].liveCoverUrl + '" width="130" height="90" alt="' + alttitle + '" /><div class="icon"></div>' + live_snip_html + '<div class="usercount"><span>' + newData[i].onlineUserCount + '</span><i></i></div></div><div class="title">' + newData[i].anchorExtendTitle + '</div></a></li>');        
            };
            $('#js-ep-bobo').innerHTML = html.join('');
        }
    }
